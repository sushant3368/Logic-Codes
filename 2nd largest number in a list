# Getting the 2nd largest number in a list.

lis = [20,25,11,7,15,3,9,21,19]
sorted_lis = []

while len(lis) != 0:
    min_lis = lis[0]
    
    for i in range(1,len(lis)):
        if lis[i] < min_lis:
            min_lis = lis[i]
            
    sorted_lis.append(min_lis)
    lis.remove(min_lis)
    
print(sorted_lis[-2])
