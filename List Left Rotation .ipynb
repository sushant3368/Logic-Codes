    "# Rotate the list to the left given number of times\n",
    "\n",
    "a = list(range(10))\n",
    "num_rot = 4   # number of times the list is to be rotated\n",
    "\n",
    "for i in range(num_rot):\n",
    "    temp = a[0]\n",
    "    del a[0]\n",
    "    a.append(temp)\n",
    "print(a)"
